{
    "gitAuthor": "nateshannon-io <114709020+nateshannon-io@users.noreply.github.com>",
    "baseBranches": ["main"],
    "rebaseWhen": "conflicted",
    "labels": ["dependencies"],
    "automergeStrategy": "merge-commit",
    "ignoreTests": true,
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "commitMessageTopic": "{{depName}}",
    "commitMessageExtra": "to {{newVersion}}",
    "commitMessageSuffix": "",
    "extends": [
      "config:base",
      ":disableRateLimiting",
      ":automergeDigest",
      ":automergeMinor",
      ":enablePreCommit",
      "docker:enableMajor",
      "docker:pinDigests"
    ],
    "terraform": {                                                                
      "depTypes": ["helm_release"]                                               
    },
    "regexManagers": [
      {
        "fileMatch": ["\\.tf$"],
        "matchStrings": [
          "default = {\\n    repo    = \"(?<registryUrl>.*?)\"\\n    version = \"(?<currentValue>([0-9]+).([0-9]+).([0-9]+))\"\\n    path    = \"(?<depName>[a-z].+)\""
        ],
        "datasourceTemplate": "helm"
      }
    ],
    "hostRules": [
      {
        "hostType": "terraform-module",
        "matchHost": "app.terraform.io",
        "encrypted": {
            "token": ""
        }
      }
    ],
    "packageRules": [
      {
        "matchUpdateTypes": ["major"],
        "labels": ["type/major"]
      },
      {
        "matchUpdateTypes": ["minor"],
        "labels": ["type/minor"]
      },
      {
        "matchUpdateTypes": ["patch"],
        "labels": ["type/patch"]
      },
      {
        "matchUpdateTypes": ["digest"],
        "labels": ["type/digest"]
      },
      {
        "matchDatasources": ["helm"],
        "addLabels": ["renovate/helm"],
        "commitMessageTopic": "chart {{depName}}"
      },
      {
        "matchDatasources": ["docker"],
        "addLabels": ["renovate/container"],
        "commitMessageTopic": "image {{depName}}"
      },
      {
        "matchDatasources": ["galaxy", "galaxy-collection"],
        "addLabels": ["renovate/ansible"]
      },
      {
        "matchDatasources": ["terraform-provider", "terraform-module"],
        "addLabels": ["renovate/terraform"]
      },
      {
        "matchDatasources": ["github-releases", "github-tags"],
        "addLabels": ["renovate/github-release"]
      },
      {
        "matchManagers": ["github-actions"],
        "addLabels": ["renovate/github-action"]
      }
    ]
}
  
